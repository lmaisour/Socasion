<%= form_for(@event) do |f| %>
<%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.text_area :title, placeholder: "Title of event" %>
    <%= f.text_area :description, placeholder: "more info" %>
    <%= f.datetime_select :whenz, placeholder: "time" %>
    <%= f.text_area :location, placeholder: "where" %>
  </div>
  <%= f.submit "Post", class: "btn btn-primary" %>
<% end %>